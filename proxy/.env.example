# LessCMS Proxy Configuration
# Copy this file to .env and fill in your values

# Proxy server port
PROXY_PORT=3001

# LessCMS API configuration
LESSCMS_API_URL=https://api.lesscms.io
LESSCMS_API_KEY=your-api-key-here
LESSCMS_WORKSPACE=your-workspace-code
LESSCMS_PROJECT=your-project-code

# Allowed origins for CORS (comma-separated)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173
