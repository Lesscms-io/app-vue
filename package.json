{
  "name": "@lesscms/vue-widgets",
  "version": "1.0.0",
  "description": "Vue 3 widget renderer library for LessCMS",
  "type": "module",
  "main": "./dist/lesscms-vue.umd.cjs",
  "module": "./dist/lesscms-vue.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lesscms-vue.js",
      "require": "./dist/lesscms-vue.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./styles/variables.css": "./src/styles/variables.css",
    "./styles/widgets.css": "./src/styles/widgets.css",
    "./styles": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:lib": "vue-tsc && vite build --mode lib",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "engine": "vite --config vite.engine.config.js"
  },
  "dependencies": {
    "@unhead/vue": "^1.9.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vue-tsc": "^1.8.0"
  },
  "peerDependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "lesscms",
    "cms",
    "widgets",
    "headless"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lesscms/vue-widgets"
  }
}
