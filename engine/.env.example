# LessCMS API Configuration
# Copy this file to .env and fill in your values

# API Base URL
VITE_LESSCMS_API_URL=https://api.lesscms.io

# API Key - get it from LessCMS dashboard
VITE_LESSCMS_API_KEY=your-api-key-here

# Workspace code
VITE_LESSCMS_WORKSPACE=your-workspace

# Project code
VITE_LESSCMS_PROJECT=your-project

# Default language (pl, en, de, etc.)
VITE_LESSCMS_LANGUAGE=pl
